{
    "name": "leadingfellows/drupal_starter_kit_profile",
    "description": "Drupal starterkit profile",
    "type": "drupal-profile",
    "license": "GPL-2.0+",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "~4.8",
        "drupal/coder": "~8.2.0",
        "drupal/devel": "^1.0",
        "squizlabs/php_codesniffer": "2.*",
        "se/selenium-server-standalone": "^2.53",
        "grasmash/yaml-cli": "^1.0"
    },
    "require": {
        "bower-asset/cropper": "~2.0",
        "bower-asset/dropzone": "~5.0",
        "drupal/admin_toolbar": "~1.0",
        "drupal/adminimal_admin_toolbar": "~1.0",
        "drupal/adminimal_theme": "~1.0",
        "drupal/advagg": "~3.0",
        "drupal/allowed_formats": "~1.0",
        "drupal/better_exposed_filters": "~3.0",
        "drupal/big_pipe_sessionless": "~1.0",
        "drupal/ckeditor_uploadimage": "~1.0",
        "drupal/config_filter": "~1.0",
        "drupal/config_ignore": "~2.0",
        "drupal/config_partial_export": "~1.4",
        "drupal/config_split": "~1.0",
        "drupal/config_update": "~1.0",
        "drupal/contact_storage": "~1.0",
        "drupal/content_access": "~1.0",
        "drupal/csv_serialization": "~1.0",
        "drupal/diff": "~1.0",
        "drupal/draggableviews": "~1.0",
        "drupal/dropzonejs": "~2.0",
        "drupal/drush_language": "1.x-dev",
        "drupal/ds": "~3.0",
        "drupal/editor_advanced_link": "~1.0",
        "drupal/editor_file": "~1.0", 
        "drupal/embed": "~1.0",
        "drupal/entity_block": "~1.0",
        "drupal/entity_embed": "~1.0",
        "drupal/field_group": "~3.0",
        "drupal/hook_post_action": "~1.0",
        "drupal/image_effects": "~2.0",
        "drupal/image_widget_crop": "~2.0",
        "drupal/imagemagick": "~2.0",
        "drupal/js_confirm_pop_up": "~1.0",
        "drupal/jsonapi": "~1.0",
        "drupal/inline_entity_form": "~1.0",
        "drupal/libraries": "3.x-dev",
        "drupal/mailsystem": "~4.0",
        "drupal/menu_block": "~1.0",
        "drupal/media_entity": "~2.0",
        "drupal/media_entity_document": "1.x-dev",
        "drupal/media_entity_image": "1.x-dev",
        "drupal/metatag": "~1.0",
        "drupal/minifyhtml": "~1.0",
        "drupal/override_node_options": "~2.0",
        "drupal/openapi": "~1.0",
        "drupal/paragraphs": "~1.0",
        "drupal/pathauto": "~1.0",
        "drupal/permissions_filter": "~1.0",
        "drupal/private_files_download_permission": "~1.0",
        "drupal/rabbit_hole": "~1.0",
        "drupal/rdfui": "~1.0",
        "drupal/redirect": "~1.0",
        "drupal/scheduler": "~1.0",
        "drupal/schema_metatag": "~1.0",
        "drupal/smart_trim": "~1.0",
        "drupal/swiftmailer": "~1.0",
        "drupal/taxonomy_menu": "~3.0",
        "drupal/twig_tweak": "~2.0",
        "drupal/video_filter": "~1.0",
        "drupal/views_data_export": "~1.0",
        "drupal/views_extras": "~1.0",
        "drupal/views_infinite_scroll": "~1.0",
        "drupal/views_slideshow": "~4.0",
        "drupal/webform": "~5.0",
        "drupal/weight": "~3.0",
        "drupal/yoast_seo": "~1.0",
        "leadingfellows/drupal_starter_kit_module": "dev-master"
    },
    "extra": {
        "patches": {
            "drupal/content_access": {
                "custom content_access patch": "patches/content_access_content_access_get_role_gid.patch"
            },
            "drupal/media_entity": {
                "2918172 - Media Entity upgrade -> core fails on absent column revision_uid":
                "https://www.drupal.org/files/issues/2918172-5.patch"
            },
            "drupal/entity_embed": {
                "2832504 - Send the CKEditor instance ID to the embed.preview route":
                "https://www.drupal.org/files/issues/2832504-2.patch"
            },
            "drupal/entity_block": {
                "2844068 - Create derivatives for any viewable entity.":
                "https://www.drupal.org/files/issues/entity_block-view-builder-class.patch",
                "2846004 - Entity Block does not perform any access control":
                "https://www.drupal.org/files/issues/2846004-2.patch"
            }
        }
    }
}
